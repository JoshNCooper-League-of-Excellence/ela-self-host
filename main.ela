import fmt::*;
import interned::*;
import fs::*;
import lexer::*;
import error::*;
import ast::*;


main :: fn() {
  mut args := Env::args();
  mut filename: str;

  if args.length >= 2 {
    filename = args[1];
  } else {
    filename = "ela_source/main.ela";
  }
  printlnf("compiling \033[1;3;32m\"%\"", (filename,));
  mut state := State::from_file(filename);
  mut lexer := Lexer::new();
  lexer.push_state(state);

  while !lexer.peek().is_type(TType::Eof) {
    lexer.eat();
  }
}
